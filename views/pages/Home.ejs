<!DOCTYPE html>
<html lang="en">

  <%- include('head')%>

<body>
  <main>
    <section>
      <div style="background: url(https://images.pexels.com/photos/1763075/pexels-photo-1763075.jpeg)"
        class="page-holder bg-cover">

        <nav class="nav1">
          <img class="logo" src="/images/logo3.png" alt="">

          <ul>

            <li> <a href="/aboutus"> About Us </a></li>

          </ul>
        </nav>

        <button id='signupBtn'>Register</button>
        <button id='loginBtn'>Login </button>


        <div class="form-style-8">

          <section id="updateSingUp">
            <form action="/singUp" method="POST" id="signup" class="signup">
              <input class="inputHome" type="text" name="name" id="name" placeholder="User Name">
              <input class="inputHome" type="email" name="email" id="email" placeholder="Email">
              <input class="inputHome" type="password" name="pass1" id="pass1" placeholder="your password">
              <input class="inputHome" type="password" name="pass2" id="pass2" placeholder="confirm your password">
              <input class="inputHome" type="submit" value="SignUp">
            </form>
          </section>
        </div>


        <div class="form-style-8">

          <section id="updateLogin">
            <form action="/login" method="POST" id="login" class="login">
              <input class=" inputHome" type="email" name="emailL" id="emailL" placeholder="email">
              <input class="inputHome" type="password" name="passwordL" id="passwordL" placeholder="password">
              <input class="inputHome" type="submit" value="Login">
            </form>
          </section>
        </div>

    </section>
  
    <div class="album">

      <h2 id="readyh2style">LATEST TRACKS </h2>

      <hr class="hr2">
      <hr class="hr">
      <hr class="hr">
      <hr class="hr1">
      <p> We sing the best <strong class="theme-color">Songs </strong>
        and now we control the world best<strong class="theme-color"> Music </strong> .
      </p>

    </div>

    <%data.map(element=>{%>
    <div class="api">
      <img src='<%=element.artist.picture_medium%>' alt="" srcset="" class=" imgData" style="border-radius: 50%;">

      <h4><%=element.title%></h4>

      <audio class="audio1" controls>
        <source src='<%=element.preview%>' type="audio/mpeg">
      </audio>
    </div>
    <% })%>



    <hr class="hr2 margintop">
    <hr class="hr">
    <hr class="hr">
    <hr class="hr1">
  </main>
  <%- include('footerhome')%>
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
  <script src="/js/app.js"></script>
</body>

</html>